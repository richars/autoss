	SEED=`tr -cd 0-9 </dev/urandom | head -c 8`
	RND_NUM=`echo $SEED 150 230|awk '{srand($1);printf "%d",rand()*10000%($3-$2)+$2}'`
	echo  $RND_NUM
  
  
